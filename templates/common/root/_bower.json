{
  "name": "<%= _.slugify(_.humanize(appname)) %>",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "angular-wakanda": "~0.4.3"<% if (ionicModule) { %>,
    "ionic": "driftyco/ionic-bower#1.0.0-beta.14"<% } %><% if (jqueryModule) { %>,
    "jquery": "~2.1.4"<% } %><% if (ngCordovaModule) { %>,
    "ngCordova": "latest"<% } %>
  },
  "appPath": "<%= appPath %>",
  "moduleName": "<%= scriptAppName %>",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ]
}